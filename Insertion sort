def insertionSort1(n, arr):
    # Write your code here
    m=arr[-1]
    for i in range(len(arr)-1,-1,-1):
        if arr[i]<arr[i-1] and i!=0:
            arr[i]=arr[i-1]
            for item in arr:
                print(item,end=" ")
            print()
            arr[i-1]=m
            
        else:
            for item in arr:
                print(item,end=" ")
            break
